{
    "name": "wayhood/hyperf-laravel",
    "type": "library",
    "keywords": [
        "php",
        "hyperf"
    ],
    "description": "A coroutine framework that focuses on hyperspeed and flexible, specifically use for build microservices and middlewares.",
    "license": "Apache-2.0",
    "require": {
        "php": ">=8.1"
    },
    "autoload": {
        "psr-4": {
            "Wayhood\\HyperfLaravel\\": "src"
        },
        "files": [
          "file/macros.php",
          "file/helpers.php"
        ]
    },
    "extra": {
        "hyperf": {
            "config": "Wayhood\\HyperfLaravel\\ConfigProvider"
        }
    }
}
